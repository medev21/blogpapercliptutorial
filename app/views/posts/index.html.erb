<% @posts.each do |post| %>
  <h2 class='text-center'><%= link_to post.title, post_path(post) %></h2>
  <% if post.iframe.present? %>
    <p class="text-center"><%= post.iframe.html_safe %></p>
  <% end %>
  <p><%= post.intro %></p><br>
  <%= link_to 'Read More', post_path(post), class: 'btn btn-default center-class' %>
<% end %>
